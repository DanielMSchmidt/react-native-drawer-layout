{
  "name": "react-native-drawer-layout",
  "version": "0.2.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run-script clear && npm run-script babel",
    "clear": "rm -rf dist/ && mkdir dist/",
    "babel": "babel *.js --out-dir dist"
  },
  "files": [
    "dist/"
  ],
  "babel": {
    "whitelist": [
      "es6.modules",
      "es6.arrowFunctions",
      "es6.blockScoping",
      "es6.classes",
      "es6.destructuring",
      "es6.parameters",
      "es6.properties.computed",
      "es6.properties.shorthand",
      "es6.spread",
      "es6.templateLiterals",
      "es6.constants",
      "es7.trailingFunctionCommas",
      "es7.objectRestSpread",
      "react",
      "es7.classProperties",
      "es7.asyncFunctions",
      "es7.decorators"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:iodine/react-native-drawer-layout.git"
  },
  "author": "Brent Vatne <brentvatne@gmail.com> (https://github.com/brentvatne)",
  "contributors": [
    {
      "name": "Marc Mendiola",
      "email": "marc@iodine.com"
    }, {
      "name": "Daniel Schmidt",
      "email": "daniel.maximilian@gmx.net",
      "url": "http://danielmschmidt.de"
    }
  ],
  "keywords": [
    "react",
    "react-native",
    "ios",
    "android",
    "drawer",
    "layout",
    "menu"
  ],
  "license": "MIT",
  "devDependencies": {
    "autobind-decorator": "^1.3.2",
    "babel": "^5.8.34"
  }
}
